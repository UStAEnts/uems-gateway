  "bugs": {
    "url": "https://github.com/ents-crew/uems-gateway/issues"
  },
  "homepage": "https://github.com/ents-crew/uems-gateway#readme",
  "dependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/helmet": "0.0.47",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.14",
    "@types/passport": "^1.0.4",
    "@uems/micro-builder": "1.0.3",
    "@uems/uemscommlib": "0.1.0-beta.43",
    "ajv": "^6.12.3",
    "amqplib": "^0.7.1",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
