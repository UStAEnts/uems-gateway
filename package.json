    "eslint-plugin-react": "^7.20.2",
    "jest": "^26.6.3",
    "jest-express": "^1.12.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.6"
  }
