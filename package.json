    "express": "^4.17.1",
    "express-openid-connect": "^2.1.0",
    "express-session": "^1.17.1",
    "helmet": "^4.3.1",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "keycloak-connect": "^12.0.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "zod": "^1.11.11"
