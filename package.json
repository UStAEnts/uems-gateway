{
  "name": "uems-gateway",
  "version": "0.0.1",
  "description": "API gateway for the UEMS system",
  "main": "index.js",
  "scripts": {
    "clean": "rm -r build/ || true",
    "build": "tsc --listEmittedFiles",
    "start": "node -r source-map-support/register --trace-warnings ./build/src/app.js",
    "start:dev": "ts-node --files src/app.ts",
    "lint": "eslint --ext .ts ./",
    "test": "jest",
    "dockerless": "UEMS_HEALTHCHECK=7783 NODE_ENV=dev npm run start:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ents-crew/uems-gateway.git"
  },
